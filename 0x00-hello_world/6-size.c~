#include <stdio.h>

/**
* main - prints the size of various types
* Return: 0 if exited properly, non-zero otherwise
*/
int main(void)
{
printf("Size of a char: %d byte(s)\n",sizeof(char));
printf("Size of an int: 4 byte(s)\n",sizeof(char));
printf("Size of a long int: 4 byte(s)\n",size(long int));
printf("Size of a long long int: 8 byte(s)\n",sizeof(long int int));
printf("Size of a float: 4 byte(s)\n",sizeof(float));
return (0);
}
